<?xml version="1.0" encoding="utf-8"?>
<SharedDataSet xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/shareddatasetdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <DataSet Name="DataSet1">
    <Query>
      <DataSourceReference>HigherED_DW</DataSourceReference>
      <CommandText>SELECT
  SUM(FactEnrollmentDetails.Enrolled) AS Enrollment
  ,DimDate.SchoolYear
FROM
  DimDate
  INNER JOIN FactEnrollmentDetails
    ON DimDate.DateSK = FactEnrollmentDetails.EnrollmentTermDateSK
GROUP BY
  DimDate.SchoolYear</CommandText>
      <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
    </Query>
    <Fields>
      <Field Name="Enrollment">
        <DataField>Enrollment</DataField>
        <rd:TypeName>System.Int32</rd:TypeName>
      </Field>
      <Field Name="SchoolYear">
        <DataField>SchoolYear</DataField>
        <rd:TypeName>System.Int32</rd:TypeName>
      </Field>
    </Fields>
  </DataSet>
  <rd:ReportServerUrl>http://localhost/ReportServer</rd:ReportServerUrl>
</SharedDataSet>